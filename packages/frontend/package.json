{
    "name": "@squeeth/frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "codegen:uniswap": "apollo client:codegen --target typescript --globalTypesFile=types/global_apollo.ts --includes=./src/queries/uniswap/**/*.ts  --endpoint 'https://api.thegraph.com/subgraphs/name/kmkoushik/uniswap-v3-ropsten'",
        "codegen:squeeth": "apollo client:codegen --target typescript --globalTypesFile=types/global_apollo.ts --includes=./src/queries/squeeth/**/*.ts  --endpoint 'https://api.thegraph.com/subgraphs/name/opynfinance/squeeth-ropsten'",
        "test:unite": "jest --watch",
        "test:e2e": "start-server-and-test 'yarn build && yarn start' http-get://localhost:3000 'synpress run'",
        "test:e2e:only:tests": "synpress run",
        "test:e2e:open:testrunner": "synpress open",
        "test:e2e:ci": "synpress run --group smoke-tests --record --parallel"
    },
