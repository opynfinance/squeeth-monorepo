        "axios": "^0.24.0",
        "bignumber.js": "^9.0.1",
        "bnc-notify": "^1.9.1",
        "bnc-onboard": "1.27.0",
        "chart.js": "^2.9.4",
        "clsx": "^1.1.1",
        "copy-to-clipboard": "^3.3.1",
        "date-fns": "^2.28.0",
        "fathom-client": "^3.2.0",
        "firebase": "^8.8.0",
        "framer-motion": "^5.3.3",
        "fzero": "^0.2.4",
        "graphql": "^16.1.0",
        "gray-matter": "^4.0.3",
        "highcharts": "^10.2.1",
        "highcharts-react-official": "^3.1.0",
        "javascript-time-ago": "^2.5.7",
        "jotai": "^1.5.3",
        "kaktana-react-lightweight-charts": "^2.0.0",
        "lodash": "^4.17.21",
        "lodash.isequal": "^4.5.0",
        "markdown-to-jsx": "^7.1.5",
        "next": "^12.0.7",
        "react": "17.0.2",
        "react-chartjs-2": "^2.11.1",
        "react-dom": "17.0.2",
        "react-query": "^3.34.0",
        "react-use": "^17.3.2",
        "set-interval-async": "^2.0.3",
        "subscriptions-transport-ws": "^0.11.0",
        "web3": "^1.5.2"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.16.7",
        "@sentry/nextjs": "^7.10.0",
        "@synthetixio/synpress": "^1.1.1",
